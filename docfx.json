{
  "build": {
    "content": [
      {
        "files": [
          "connector/**.md",
          "fonts/**",
          "**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "sitemap": {
      "baseUrl": "https://docs.dataminer.services",
      "changefreq": "hourly"
    },
  "resource": [
      {
        "files": [
          "connector/images/**",
          "staticwebapp.config.json",
          "404.html"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "DataMiner Docs",
      "_appFooter": "Â© Skyline Communications 2025",
      "_enableSearch": false,
      "_enableNewTab": true,
	  "_googleAnalyticsTagId": "GTM-WD3NQGN"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
	  "modern",
      "templates/skyline"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
